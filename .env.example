# US Stocks Autobot Configuration

# Debug Mode - Enable detailed logging (true/false)
DEBUG_MODE=false

# Stock Selection Settings
# Maximum number of stocks to select
TOP_K=12

# Historical data lookback period (days)
LOOKBACK_DAYS=120

# Custom stock list (optional)
# If not set, uses built-in list of 200+ major US stocks
# Format: comma-separated ticker symbols
# US_STOCK_CODES=AAPL,MSFT,GOOGL,AMZN,META,NVDA,TSLA,AMD,INTC,QCOM

# Data Directory (default: data)
DATA_DIR=data

# Output Directory (default: output)
OUTPUT_DIR=output
